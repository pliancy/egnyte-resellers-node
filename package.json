{
  "name": "egnyte-resellers",
  "version": "1.2.7",
  "description": "Library for managing things against the undocumented egnyte resellers API.",
  "homepage": "https://github.com/pliancy/egnyte-resellers-node",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf coverage dist tmp",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "build": "yarn clean && yarn test && tsc -p tsconfig.release.json",
    "build:check": "tsc --noEmit -p tsconfig.release.json",
    "qa": "yarn lint && yarn test && yarn clean && yarn build:check",
    "test": "jest",
    "postinstall": "[ -d '.husky' ] && husky install || true",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "egnyte",
    "resellers",
    "msp"
  ],
  "author": "Josh Stout <joshstout@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zenion/egnyte-resellers-node"
  },
  "dependencies": {
    "axios": "0.21.1"
  },
  "devDependencies": {
    "@pliancy/eslint-config-ts": "0.0.5",
    "@types/jest": "26.0.24",
    "@types/node": "14.17.5",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "husky": "4.3.8",
    "jest": "27.0.6",
    "lint-staged": "10.5.4",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.3",
    "ts-node": "9.1.1",
    "typescript": "4.3.5"
  },
  "volta": {
    "node": "14.15.4",
    "yarn": "1.22.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
