{
  "name": "egnyte-resellers",
  "version": "1.2.5",
  "description": "Library for managing things against the undocumented egnyte resellers API.",
  "homepage": "https://github.com/pliancy/egnyte-resellers-node",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf coverage dist tmp",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "build": "npm run clean && npm run test && tsc -p tsconfig.release.json",
    "build:check": "npm run clean && npm run test && tsc --noEmit -p tsconfig.release.json",
    "test": "npm run lint && jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["egnyte", "resellers", "msp"],
  "author": "Josh Stout <joshstout@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zenion/egnyte-resellers-node"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^5.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "volta": {
    "node": "14.15.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build:check && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": ["eslint --fix", "prettier --write"]
  }
}
